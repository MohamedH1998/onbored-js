{
  "name": "onbored-js",
  "version": "0.1.0-alpha.5",
  "description": "SDK for OnBored",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "check-all": "pnpm run type-check && pnpm run lint:check && pnpm run format:check",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "pnpm run test && pnpm run test:e2e",
    "start:test": "node tests/utils/test-server.js"
  },
  "dependencies": {
    "@rrweb/types": "2.0.0-alpha.18",
    "pako": "^2.1.0",
    "rrweb": "2.0.0-alpha.4",
    "uuid": "^11.1.0",
    "zod": "^3.25.64"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.36.0",
    "@mswjs/interceptors": "^0.39.7",
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^22.15.29",
    "@types/pako": "^2.0.3",
    "@types/react": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-resolve": "^30.2.0",
    "lint-staged": "^16.2.3",
    "msw": "^2.11.3",
    "playwright": "^1.40.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "18.3.1",
    "ts-jest": "^29.4.4",
    "ts-jest-resolver": "^2.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "onbored",
    "session-replay",
    "onboarding",
    "analytics"
  ],
  "license": "MIT",
  "packageManager": "pnpm@10.9.0",
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{js,jsx,json}": [
      "prettier --write"
    ]
  }
}
